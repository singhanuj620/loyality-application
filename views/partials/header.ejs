	<script type="text/javascript">
    var time=2000;
    var images=[];
    var i=0;
    images[0]="https://static.digit.in/default/2bd0c7c9514ad1c5f0f133afd03cbf4c5ae2f757.jpeg";
    images[1]="https://static.digit.in/default/4f2588a7b4e6bba52ae133a5609a4550d83f4987.jpeg";
