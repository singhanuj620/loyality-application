    images[4]="https://static.digit.in/default/74aca9660d20107ee891556f01ea0a1b8bab2e15.jpeg";
    images[5]="https://static.digit.in/default/81cebdb3dde9ddce404ace83ee147af9d1eca1f4.jpeg";

    function changeImg(){
        document.slide.src=images[i];
        if(i<images.length-1)
        {
            i++;
        }
        else{
            i=0;
        }

        setTimeout("changeImg()",time);
    }

    window.onload=changeImg;
</script>

